#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/arnogau/iudex-node-module/node_modules/.pnpm/bun@1.0.26/node_modules/bun/bin/node_modules:/Users/arnogau/iudex-node-module/node_modules/.pnpm/bun@1.0.26/node_modules/bun/node_modules:/Users/arnogau/iudex-node-module/node_modules/.pnpm/bun@1.0.26/node_modules:/Users/arnogau/iudex-node-module/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/arnogau/iudex-node-module/node_modules/.pnpm/bun@1.0.26/node_modules/bun/bin/node_modules:/Users/arnogau/iudex-node-module/node_modules/.pnpm/bun@1.0.26/node_modules/bun/node_modules:/Users/arnogau/iudex-node-module/node_modules/.pnpm/bun@1.0.26/node_modules:/Users/arnogau/iudex-node-module/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../bin/bun"   "$@"
exit $?
