#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/arnogau/iudex-node-module/node_modules/.pnpm/tsup@8.0.1_ts-node@10.9.2_typescript@5.3.3/node_modules/tsup/dist/node_modules:/Users/arnogau/iudex-node-module/node_modules/.pnpm/tsup@8.0.1_ts-node@10.9.2_typescript@5.3.3/node_modules/tsup/node_modules:/Users/arnogau/iudex-node-module/node_modules/.pnpm/tsup@8.0.1_ts-node@10.9.2_typescript@5.3.3/node_modules:/Users/arnogau/iudex-node-module/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/arnogau/iudex-node-module/node_modules/.pnpm/tsup@8.0.1_ts-node@10.9.2_typescript@5.3.3/node_modules/tsup/dist/node_modules:/Users/arnogau/iudex-node-module/node_modules/.pnpm/tsup@8.0.1_ts-node@10.9.2_typescript@5.3.3/node_modules/tsup/node_modules:/Users/arnogau/iudex-node-module/node_modules/.pnpm/tsup@8.0.1_ts-node@10.9.2_typescript@5.3.3/node_modules:/Users/arnogau/iudex-node-module/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli-node.js" "$@"
else
  exec node  "$basedir/../../dist/cli-node.js" "$@"
fi
